s\n          })\n        },\n        (error) => {\n          this.setState({\n            isLoaded: true,\n            error\n          })\n        }\n      )\n    }\n  \n    render() {\n    const movieList = this.state.movies.map(({ title, poster_path, id}, i) => {\n        return (\n            <Movie \n                id={id}\n                key={title}\n                title={title}\n                image={poster_path}\n            />\n        )\n    })\n      const { error, isLoaded, movies } = this.state\n  \n      if (error) {\n        return <div>Error: {error.message} </div>\n      } else if (!isLoaded) {\n        return <div>Loading...</div>\n      } else {\n        return (\n          <div className=\"MovieList\">\n                {movieList}\n          </div>\n        )\n      }\n    }\n  \n  }\n  \n\n\nexport default MovieList;","/Users/caomai/dev/Courses/term7/few1.2/react-themoviedb/src/components/movie/Movie.js",["45"],"import React from 'react';\nimport { Link } from 'react-router-dom'\n\nfunction Movie(props) {\n    const { title, id, image } = props\n\n    return (\n        <div className=\"Movie\">\n            <div className=\"Movie-info\">\n                <Link to={`/details/${id}`}>\n\n                {/* <div>{title}</div> */}\n                <img src={\"http://image.tmdb.org/t/p/w500/\"+image} width=\"100%\" alt=\"space_pic\" />\n                </Link>         \n            </div>\n\n        </div>\n    )\n}\n\nexport default Movie","/Users/caomai/dev/Courses/term7/few1.2/react-themoviedb/src/components/movie-details/MovieDetails.js",["46"],"import React, { Component } from \"react\";\nimport Movie from \"../movie/Movie\";\n\nclass MovieDetails extends Component {\n  constructor(props) {\n    super(props);\n    const { id } = props.match.params; // Location index\n\n    console.log(id);\n    this.state = {\n      id: id,\n      isLoaded: false,\n      result: null,\n    };\n  }\n\n  componentDidMount() {\n    // console.log(this.state.result);\n\n    fetch(\n      `https://api.themoviedb.org/3/movie/${this.state.id}?api_key=33cff2b0fc5d808790ffb3ba68053642&language=en-US`\n    )\n      .then((res) => res.json())\n      .then((result) => {\n        this.setState({\n          isLoaded: true,\n          result: result,\n        });\n        // console.log(this.state.result.title);\n      });\n  }\n\n  render() {\n    const { isLoaded, result } = this.state;\n    if (!isLoaded) {\n      return <div>Loading...</div>;\n    } else {\n      return (\n        <div className=\"movie-detail-section\">\n          <h1 className=\"movie-title\">{result.title}</h1>\n          <img\n            src={\"http://image.tmdb.org/t/p/w500/\"+result.poster_path}\n            width='40%'\n            alt='space_pic'\n          />\n          <p className=\"movie-description\">{result.overview}</p>\n          <a href={`${result.homepage}`}>Movie Homepage</a>\n\n        </div>\n      );\n    }\n  }\n}\n\nexport default MovieDetails;\n","/Users/caomai/dev/Courses/term7/few1.2/react-themoviedb/src/components/footer/Footer.js",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":49,"column":32,"nodeType":"53","messageId":"54","endLine":49,"endColumn":38},{"ruleId":"51","severity":1,"message":"55","line":5,"column":13,"nodeType":"53","messageId":"54","endLine":5,"endColumn":18},{"ruleId":"51","severity":1,"message":"56","line":2,"column":8,"nodeType":"53","messageId":"54","endLine":2,"endColumn":13},"no-native-reassign",["57"],"no-negated-in-lhs",["58"],"no-unused-vars","'movies' is assigned a value but never used.","Identifier","unusedVar","'title' is assigned a value but never used.","'Movie' is defined but never used.","no-global-assign","no-unsafe-negation"]